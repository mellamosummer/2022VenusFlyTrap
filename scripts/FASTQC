#FASTQC Script
#!/bin/bash
#SBATCH --job-name=FastQC
#SBATCH --partition=batch
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=4
#SBATCH --mem=50gb
#SBATCH --time=7:00:00
#SBATCH --output=%j.out
#SBATCH --error=%j.error

module load FastQC/0.11.9-Java-11

fastqc -o /scratch/srb67793/VFT_Analysis2/2_FastQC_VFT/results /scratch/srb67793/VFT_Analysis2/Raw_reads/*.fastq
